{
    "name": "streams/testing",
    "description": "Testing for Streams",
    "type": "streams-addon",
    "license": "MIT",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Ryan Thompson",
            "email": "ryan@pyrocms.com"
        }
    ],
    "require": {
        "streams/core": "^2.0",
        "brianium/paratest": "^6.4",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Streams\\Testing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Streams\\Testing\\Tests\\": "tests/"
        }
    },
    "post-autoload-dump": [
        "@php artisan package:discover --ansi"
    ],
    "extra": {
        "branch-alias": {
            "dev-1.0": "1.x-dev"
        }
    }
}
